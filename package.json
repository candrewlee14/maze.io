{
  "name": "maze.io",
  "version": "1.0.0",
  "description": "A MMO maze game",
  "main": "server/app.js",
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "socket.io": "^2.4.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2"
  },
  "scripts": {
    "server_build": "cd server && npm run build",
    "client_build": "cd client && npm run build",
    "server_start": "cd server && npm run start",
    "client_start": "cd client && npm run serve",
    "install_all": "npm install && cd server && npm install && cd ../client && npm install",
    "start_all": "npm run-all -p server_start client_start",
    "build_all": "npm-run-all -p server_build client_build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/candrewlee14/maze.io.git"
  },
  "author": "Andrew Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/candrewlee14/maze.io/issues"
  },
  "homepage": "https://github.com/candrewlee14/maze.io#readme"
}
